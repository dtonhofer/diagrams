            <node id="n0::n18::n1">
              <data key="d6">
                <y:GenericNode configuration="com.yworks.flowchart.process">
                  <y:Geometry height="655.4883686142421" width="456.13999453788574" x="7211.266950671981" y="-2505.901721379013"/>
